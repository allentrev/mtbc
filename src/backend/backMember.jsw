export async function getAllGoogleMembers() {
    try {
        let wResult = await wixData
            .query("lstGoogleImport")
            .ascending("lastName")
            .ascending("firstName")
            .limit(1000)
            .find();

        const wRecords = wResult.items;
        //console.log(wResult);

        let wAllGoogleMembers = [];

        if (wRecords.length > 0) {
            let wCouples = wRecords.filter(
                (item) => item.firstName && item.firstName.includes("&")
            );
            let wSingles = wRecords.filter(
                (item) => item.firstName && !item.firstName.includes("&")
            );
            //console.log(wRecords.length);
            //console.log(wCouples);
            //console.log(wSingles);
            let wUpdatedSingles = await setGenderInSingles(wSingles);
            //console.log("UpdatedSingles");
            //console.log(wUpdatedSingles);
            let wMergedImportSet = await addCouplesSetToImportSet(
                wUpdatedSingles,
                wCouples
            );
            //console.log("wMergedImportSet");
            //console.log(wMergedImportSet);

            wAllGoogleMembers = formatImportSet(wMergedImportSet);
            //console.log("wAllGoogleMembers");
            //console.log(wAllGoogleMembers);
        }
        return wAllGoogleMembers;
    } catch (err) {
        console.log("/backend/BackMember getAllGoogleMembers Try-catch err");
        console.log(err);
        return [];
    }
}

async function setGenderInSingles(pMembers) {
    let wSinglesSet = [];
    for (let wMember of pMembers) {
        let wLabels = wMember.labels;
        if (wLabels && wLabels.includes("::: Men")) {
            wMember.gender = "M";
        } else if (wLabels && wLabels.includes("::: Ladies")) {
            wMember.gender = "L";
        } else {
            //console.log(
            //    `backend/backMember setGenderInSingles cannot determine gender for ${wMember._id} and ${wLabels}`
            //);
            wMember.gender = await getGender(
                wMember.firstName,
                wMember.lastName
            );
        }
        wSinglesSet.push(wMember);
    }
    return wSinglesSet;
}

async function addCouplesSetToImportSet(pImport, pCouples) {
    let wImportSet = [...pImport];
    if (pCouples.length > 0) {
        // create 2 records from the 1 imported
        for (let wCouple of pCouples) {
            let wNewRecord1 = { ...wCouple };
            let wNewRecord2 = { ...wCouple };
            const [wFirstPerson, wSecondPerson] =
                wCouple.firstName.split(" & ");

            wNewRecord1.firstName = wFirstPerson;
            wNewRecord1.labelArray = formLabelArrayfromLabel(wCouple.labels);
            wNewRecord1.gender = await getGender(
                wFirstPerson,
                wCouple.lastName
            );
            wImportSet.push(wNewRecord1);
            let wResult = await saveGoogleRecord(
                "lstGoogleImport",
                wNewRecord1
            );
            if (wResult && wResult.status) {
                wNewRecord2.firstName = wSecondPerson;
                wNewRecord2._id = undefined;
                wNewRecord2.labelArray = wNewRecord1.labelArray;
                wNewRecord2.gender = await getGender(
                    wSecondPerson,
                    wCouple.lastName
                );
                wResult = await saveGoogleRecord(
                    "lstGoogleImport",
                    wNewRecord2
                );
                if (wResult && wResult.status) {
                    let wId = wResult.savedRecord._id;
                    wNewRecord2._id = wId;
                    wImportSet.push(wNewRecord2);
                } else {
                    console.log(
                        `/backend/backMember getAllGoogleMembers second save: couple (${wFirstName}) record failed`
                    );
                    return;
                }
            } else {
                console.log(
                    `/backend/backMember getAllGoogleMembers first save: couple (${wFirstName}) record failed`
                );
                return;
            }
        } // for loop
    } // couples > 0

    let wSortedImportSet = _.orderBy(wImportSet, ["lastName", "firstName"]);

    return wSortedImportSet;
}

async function getGender(pFirstName, pLastName) {
    let wGender = "M";
    let wResult = await findLstMemberByFullName(pFirstName, pLastName);
    if (wResult && wResult.status) {
        let wMember = wResult.members[0];
        wGender = wMember.gender;
    }
    return wGender;
}

function formatImportSet(pMembers) {
    let wFormattedSet = [];

    for (let wMember of pMembers) {
        let wNewItem = {};
        // format mobile phone number
        let wPhone = wMember.phone1Value;
        let wTrimmedPhone = null;
        let wMobilePhone = null;
        if (wPhone === " " || wPhone === null || wPhone === undefined) {
            wMobilePhone = null;
        } else {
            wTrimmedPhone = wPhone.trim();
            wMobilePhone = wTrimmedPhone.replace(/\s+/g, "");
        }
        // format names
        let wFirstName = wMember.firstName ? wMember.firstName.trim() : "";
        let wLastName = wMember.lastName ? wMember.lastName.trim() : "";
        let wFullName = wFirstName + " " + wLastName;
        // process Labels
        let wLabelArray = formLabelArrayfromLabel(wMember.labels);
        // Output new Member
        wNewItem.key = wFullName;
        wNewItem._id = wMember._id;
        wNewItem.firstName = wFirstName;
        wNewItem.lastName = wLastName;
        wNewItem.surname = wLastName;
        wNewItem.gender = wMember.gender;
        wNewItem.player = wFullName;
        wNewItem.fullName = wFullName;
        wNewItem.eMail1Label = wMember.eMail1Label || null;
        wNewItem.eMail1Value =
            wMember.eMail1Value ? wMember.eMail1Value.toLowerCase() : null;
        wNewItem.eMail2Label = wMember.eMail2Label || null;
        wNewItem.eMail2Value =
            wMember.eEmail2Value ? wMember.eMail2Value.toLowerCase() : null;
        wNewItem.phone1Label = wMember.phone1Label || null;
        wNewItem.phone1Value = wMobilePhone || null;
        wNewItem.labels = wMember.labels || null;
        wNewItem.labelArray = wLabelArray || [];

        wFormattedSet.push(wNewItem);
    }

    return wFormattedSet;
}

function formLabelArrayfromLabel(pLabels) {
    let wLabelArray = [];
    if (pLabels && pLabels.length > 0) {
        wLabelArray = pLabels.split(" ::: ");
        wLabelArray.pop(); // remove last entry which is always * myContacts
    }

    return wLabelArray;
}

