// Helper function to find unique elements in an array
const unique = (array, ...excludeArrays) => {
    return array.filter(
        (item) =>
            !excludeArrays.some((excludeArray) =>
                excludeArray.some((excludeItem) => excludeItem.key === item.key)
            )
    );
};
