export async function getActiveWixMembers() {
    let wWixMembers = await wixData
        .query("Members/PrivateMembersData")
        .ne("status", "APPLICANT")
        .ne("status", "BLOCKED")
        .ascending("lastName")
        .ascending("firstName")
        .limit(1000)
        .find();
    return wWixMembers.items;
