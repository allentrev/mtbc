export function registerWixMember(pRec) {
    let options = {
        contactInfo: {
            firstName: pRec.firstName,
            lastName: pRec.lastName,
        },
        privacyStatus: "PUBLIC",
    };
    return authentication
        .register(pRec.email.toLowerCase(), pRec.password, options)
        .then((regResult) => {
            return [true, regResult];
        })
        .catch((err) => {
            return [false, err];
        });
}

